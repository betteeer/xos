    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.6.6</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>com.inossem.nt</groupId>
    <artifactId>oms</artifactId>
    <version>1.0</version>
    <name>oms</name>
    <description>project for Spring Boot</description>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <java.version>1.8</java.version>
        <mybatisplus.version>3.5.2</mybatisplus.version>
        <hutool.version>5.3.4</hutool.version>
        <fastjson.version>1.2.83</fastjson.version>
        <swagger3.version>3.0.0</swagger3.version>
        <knife4j.version>3.0.3</knife4j.version>
        <pagehelper.version>1.4.3</pagehelper.version>
        <page.version>5.3.1</page.version>
        <jedis.version>3.5.2</jedis.version>
        <guava.version>20.0</guava.version>
        <alibaba.druid.version>1.2.9</alibaba.druid.version>
        <shiro.version>1.6.0</shiro.version>
        <sco-msa.version>1.7.5-SNAPSHOT</sco-msa.version>
        <transmittable-thread-local.version>2.13.2</transmittable-thread-local.version>
        <poi.version>4.1.2</poi.version>
