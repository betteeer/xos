<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.inossem.oms.base.svc.mapper.DashboardChartUserRelationMapper">
    <insert id="insertBatch" parameterType="list" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO dashboard_chart_user_relation
        (
        user_id,
        chart_id,
        gmt_create,
        create_by,
        gmt_modified,
        modified_by
        )
        VALUES
        <foreach collection="list" item="item"  separator=",">
            (
            #{item.userId},
            #{item.chartId},
            #{item.gmtCreate},
            #{item.createBy},
            #{item.gmtModified},
            #{item.modifiedBy}
            )
        </foreach>
    </insert>
</mapper>